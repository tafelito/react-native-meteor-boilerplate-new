{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "rules": {
	  "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
	}
}